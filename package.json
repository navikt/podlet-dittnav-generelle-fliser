{
  "name": "podlet-template",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "5.9.0",
    "@podium/podlet": "4.4.14",
    "classnames": "2.2.6",
    "craco-less": "1.17.1",
    "express": "4.17.1",
    "less": "3.12.2",
    "msw": "0.24.1",
    "nav-frontend-core": "5.0.10",
    "nav-frontend-lenker": "1.0.53",
    "nav-frontend-lenker-style": "0.2.39",
    "nav-frontend-paneler": "2.0.22",
    "nav-frontend-paneler-style": "0.3.31",
    "nav-frontend-typografi": "2.0.37",
    "nav-frontend-typografi-style": "1.0.31",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/user-event": "12.5.0",
    "@types/jest": "26.0.16",
    "@types/node": "14.14.10",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "react-scripts": "4.0.1",
    "typescript": "4.1.2"
  },
  "scripts": {
    "start": "NODE_ENV=development craco start",
    "build": "craco build",
    "test": "craco test",
    "test-ci": "CI=true craco test",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
    "typeCheck": "tsc",
    "podlet": "node podlet.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 120
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run typeCheck && npm run test-ci"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
  }
}
